{
	"version": 2,
	"buildCommand": "corepack enable && pnpm install && pnpm run build",
	"installCommand": "corepack enable && pnpm install",
	"builds": [
		{
			"src": "dist/vercel-server.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "dist/vercel-server.js"
		}
	],
	"outputDirectory": "dist"
}
